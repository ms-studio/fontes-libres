            <ITEXT CH="] quand ni l'incorporation ni la rfrence ni la distribution incorpore ne sont autorises ;"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH=" - ["/>
            <ITEXT FONT="DejaVu Sans Oblique" CH="Document imprimable"/>

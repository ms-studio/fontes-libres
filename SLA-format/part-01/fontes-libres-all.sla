            <ITEXT FONT="DejaVu Sans Oblique" CH="Add"/>
            <ITEXT CH=") puis dans la partie inférieure sous le bouton "/>
            <ITEXT FONT="DejaVu Sans Oblique" CH="choisir une couleur de remplissage"/>
